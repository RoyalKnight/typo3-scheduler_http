{
    "name": "webentwicklerat/scheduler-http",
    "description": "Invoke scheduler via HTTP-Request and add scheduler tasks doing GET-Requests.",
    "keywords": ["typo3", "scheduler", "http"],
    "homepage": "https://github.com/WebentwicklerAt/typo3-scheduler_http",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Gernot Leitgab",
            "homepage": "https://webentwickler.at"
        }
    ],
    "require": {
        "typo3/cms": "~6.2.0 || ~7.6.0 || ~8.7.0",
        "typo3/cms-scheduler": "~6.2.0 || ~7.6.0 || ~8.7.0"
    },
    "replace": {
        "scheduler_http": "self.version",
        "typo3-ter/scheduler_http": "self.version"
    },
    "autoload": {
        "psr-4": {
            "WebentwicklerAt\\SchedulerHttp\\": "Classes/"
        }
    },
    "scripts": {
        "pack": [
            "git archive -o ../scheduler_http.zip HEAD"
        ]
    }
}